<!doctype html>
<html lang="en">
<head>
  <title>{% block title %}{% endblock %} - RECAPSE</title>
  <link rel="stylesheet" href="{{ url_for('static', filename='style.css') }}">
</head>
<body>
  <nav>
    <h1><a href="{{ url_for('views.index') }}">RECAPSE Recurrence Prediction</a></h1>
    <ul>
        <li><a href="{{ url_for('views.view_tasks') }}">Running/Completed Tasks</a>
        <li><a href="{{ url_for('views.view_preprocess') }}">Preprocess Data</a>
        <li><a href="{{ url_for('views.view_predict') }}">Run Prediction</a>
    </ul>
  </nav>
  <section class="content">
    <header>
    {% block header %}{% endblock %}
    </header>
    {% for message in get_flashed_messages() %}
      <div class="flash">{{ message }}</div>
    {% endfor %}
    {% block content %}{% endblock %}
  </section>
</body>
</html>
